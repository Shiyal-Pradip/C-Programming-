// Program To Print ASCII Value Using Entered Keyboard Key
#include<stdio.h>
#include<conio.h>
void ASCII()
{
   char Char;
   int i;
   gotoxy(24,1);
   printf("Convert Any Keyboard key in to ASCII\n");
   for(i=0; i<80; i++) printf("*");
   // Enter Any Keyboard Key
   printf("Enter Any Keyboard Key:- ");
   scanf("%c",&Char);
   printf("\nASCII Value is= %d",Char);
}
void main()
{
    void ASCII(void);
 clrscr();
    ASCII();
 getch();
}
