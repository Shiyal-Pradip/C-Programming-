#include<stdio.h>
#include<conio.h>
void Fibonacci()
{
	long Rang,i;
	long A=-1,B=1,C;
	gotoxy(21,1);
	printf("Program To Print Fibonacci Series\n");
	for(i=0; i<80; i++) printf("*");
	printf("Enter Rang:- ");
	scanf("%ld",&Rang);
	if(Rang>46)
		{
			printf("\n\n\tPlease Enter Rang Less Than orequl to 46");
		}
	else
		{
			printf("\n");
			for(i=0; i<=Rang; i++)
				{
					C=A+B;
					printf("%ld\t",C);
					A=B;
					B=C;
				}
		}
}

