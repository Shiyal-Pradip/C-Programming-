#include<stdio.h>
#include<conio.h>
void Sort_Arry()
{
   long Num[200],N;
   long i,j,a;

   printf("Enter Number Of Elements In An Array:- ");
   scanf("%ld", &N);

   printf("\nEnter %ld Elements:- \n",N);

   for(i=0; i<N; i++)
      {
	 scanf("%ld", &Num[i]);
      }
   for(i=0; i<N; i++)
      {
	 for(j=i; j<N; j++)
	    {
		if(Num[i]>Num[j])
		   {
		      a=Num[i];
		      Num[i]=Num[j];
		      Num[j]=a;
		   }
	    }
      }
   printf("\nThe numbers in ascending order is:- \n");

   for(i=0; i<N; i++)
      {
	 printf("%ld  ",Num[i]);
      }
}
void main()
{
     void Sort_Arry(void);
 clrscr();
     Sort_Arry();
 getch();
}
